# -*- tcl -*-
# # ## ### ##### ######## ############# #####################
## queue.test:  tests for the queue package.

source [file dirname [file dirname [file normalize [info script]]]]/include/utilities.tcl

testsNeedTcl     8.5
testsNeedTcltest 2 ; # 8.5 does not have tcltest 1.x

support {
    useC _local/lib/cindex* c::index
    useC _local/lib/cslice* c::slice
    useC _local/lib/cstack* c::stack
    useC _local/lib/cqueue* c::queue
}
testing {
    useC _local/lib/queuec* struct::queue
}

# # ## ### ##### ######## ############# #####################

proc already {cmd} {
    return "command \"::$cmd\" already exists, unable to create ::struct::queue instance"
}

proc badmethod {m real} {
    set real [linsert [join $real {, }] end-1 or]
    return "bad option \"$m\": must be $real"
}

::tcltest::testConstraint queue_c   1
::tcltest::testConstraint queue_tcl 0

# # ## ### ##### ######## ############# #####################

source [localPath ../include/queue.tests]

# # ## ### ##### ######## ############# #####################
testsuiteCleanup
