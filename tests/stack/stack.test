# -*- tcl -*-
# # ## ### ##### ######## ############# #####################
## stack.test:  tests for the stack package.

source [file dirname [file dirname [file normalize [info script]]]]/testutilities.tcl

testsNeedTcl     8.5
testsNeedTcltest 2 ; # 8.5 does not have tcltest 1.x

support {
    useC _test/lib/cslice* c::slice
    useC _test/lib/cstack* c::stack
}
testing {
    useC _test/lib/stackc* struct::stack
}

# # ## ### ##### ######## ############# #####################

set MY mystack

proc tmWrong {m loarg n {xarg {}}} {
    return [tcltest::wrongNumArgs "mystack $m" $loarg $n]
}

proc tmTooMany {m loarg {xarg {}}} {
    return [tcltest::tooManyArgs "mystack $m" $loarg]
}

proc tmTake {tcl c} { return $c }

# # ## ### ##### ######## ############# #####################

puts RUN__________________________________
source [localPath stack.testsuite]

# # ## ### ##### ######## ############# #####################
testsuiteCleanup
